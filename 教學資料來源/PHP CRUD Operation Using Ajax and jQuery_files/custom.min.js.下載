function onsubscribe(){var e=$("#email_id").val();return e?ValidateEmail(e)?($("#subcribeBtn").hide(),$("#subcribeLoader").show(),void $.ajax({type:"get",data:{email:e},url:"/subscribe",success:function(e){console.log(e),$("#subscribeSuccess").html(e),$("#subcribeLoader").hide(),$("#subcribeBtn").show(),$("#subscribeSuccess").show(),setTimeout(()=>{$("#subscribeSuccess").hide()},5e3)}}).fail(function(e){console.log(e);var s=e.responseJSON.errors,i=[];for(var r in s)i.push(s[r]);var a=i[0][0];$("#subscribeFail").html(a),$("#subscribeFail").show(),$("#subcribeLoader").hide(),$("#subcribeBtn").show(),setTimeout(()=>{$("#subscribeFail").hide()},5e3)})):(alert("You have entered an invalid email address!"),!1):(alert("Please enter your email id!"),!1)}function ValidateEmail(e){return!!/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(e)}$(function(){$(window).scroll(function(){$(window).scrollTop()<8795?$(".sticky-social").show():$(".sticky-social").hide()})}),$(".main-content-area p").each(function(){var e=$.trim($(this).html());"&nbsp;"==e&&($(this).removeAttr("style"),$(this).addClass("line-break"))});